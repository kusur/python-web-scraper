<script type="text/javascript">
    $(function() {

        if (SiteDTO.ID == 1) {
            //$('#header_link > ul li').eq(10).remove();
            $('.vidyaLink').hide();
        }

        if ('0' != 0) {
            $('#login').remove();
        }
        else {
            $('#logout').remove();
        }

        $('#login').click(function(e) {
            $('#SearchDiv').hide();
            $('#LoginDiv').slideDown("slow");
        });

        $('#search').click(function(e) {
            $('#LoginDiv').hide();
            $('#SearchDiv').slideDown("slow");
        });
        $('#searchBtn').click(function(e) {
            e.preventDefault();
            CallSearch();
        });

        $('#searchInput').keypress(function(e) {
            code = e.keyCode ? e.keyCode : e.which;
            if (code.toString() == 13) {
                CallSearch();
            }
        });

        $('#HeaderLoginPassword').bind('keypress', function(e) {
            code = e.keyCode ? e.keyCode : e.which;
            if (code.toString() == 13) {
                LoginAuthenticate();
            }
        });

        $('#HeaderLoginButton').click(function(e) {
            LoginAuthenticate();
        });

    });

    function LoginAuthenticate() {

        var userNameObj = $('#HeaderLoginUserName'), passwordObj = $('#HeaderLoginPassword');

        if (userNameObj.val() == "") {
            showError('HeaderMessageHolder', 'Enter User Name');
            return;
        }

        if (passwordObj.val() == "") {
            showError('HeaderMessageHolder', 'Enter Password');
            return;
        }

        showProcess('HeaderMessageHolder', 'Validating Login Please Wait...');

        var DTO = { 'username': userNameObj.val(), 'password': passwordObj.val() }, url = ResolveUrl("~/ajax/ajaxCall.aspx/Authenticate");

        callAjax({
            Type: "JSON",
            DataObject: DTO,
            AjaxUrl: url,
            CallBack: function(data) {

                var response = data.split("|");
                if (response[0] == 'success') {
                    window.location.href = response[1];
                }
                else {
                    showError('HeaderMessageHolder', 'User Not Found');
                }

            },
            ReturnJson: true
        });
    }

    function CallSearch() {
        window.location = ResolveUrl("~/" + SiteDTO.Allias + "/search/") + $('#searchInput').val();
    };
</script>


