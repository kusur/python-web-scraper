    <script type="text/javascript">
        var IS_DEFAULT_PAGE = true;

        var PageMenuDTO = {
            'MENU_ID'           : '627',        
            'PAGEID'            : '574',
            'CURRENT_MENU_ID'   : '627',
            'LANGUAGE_ID'       : '1',
            'LANGUAGE'          : 'en',
            'IS_PUBLISH_PAGE'   : 'True',
        };


        var selectedlanguage;
        switch (parseInt(PageMenuDTO.LANGUAGE_ID)) {
            case 2:
                selectedlanguage = "hindi";
                break;

            case 3:
                selectedlanguage = "gujarati";
                break;

            case 4:
                selectedlanguage = "marathi";
                break; 
                
            default:
                selectedlanguage = "english";
                break; 
        }

        var ControlJson = [];
