<script type="text/javascript">
    $(function () {
        var overSubmenu = false;
        var overTopMenu = false;
        var timeoutID;
        var closing = false;
        $("[id$='TopMenu']").hover(
            function () {
                // over         
                overTopMenu = true;
                window.clearTimeout(timeoutID);
                if (closing) {
                    $("[id$='SubMenu']").stop(true, true).show('slide', { direction: "up" }, 500);
                    closing = false;
                } else {
                    if ($("[id$='SubMenu']").css('display') == 'none')
                        $("[id$='SubMenu']").show('slide', { direction: "up" }, 500);
                }
            },
            function () {
                // out  
                overTopMenu = false;
                timeoutID = window.setTimeout(closeMenu, 100);
            }

        );
        $("[id$='SubMenu']").hover(
            function () {
                // over  
                overSubmenu = true;
                window.clearTimeout(timeoutID);
                if (closing) {
                    $("[id$='SubMenu']").stop(true, true).show('slide', { direction: "up" }, 100);
                    closing = false;
                }                
            },
            function () {
                // out               
                overSubmenu = false;
                timeoutID = window.setTimeout(closeMenu, 100);
            }

        );

        function closeMenu() {
            window.clearTimeout(timeoutID);
            if (!overSubmenu && !overTopMenu) {
                closing = true;
                $("[id$='SubMenu']").stop(true, true).hide('slide', { direction: "up" }, 100, function () {
                    closing = false;
                    $(this).removeAttr('style');
                });
            }
        }
    });
</script>

