            <!-- Footer User Control -->
            

<script type="text/javascript" language="javascript">
    $(document).ready(function() {

        $("#btnSubscribe").click(function() {
            if ($("#ucFooter_txtSubscribe").val() == "") {
                showSubError("Enter your email id");
                return false;
            }
            else if ($("#ucFooter_txtSubscribe").val() != "") {
                var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
                var address = $("#ucFooter_txtSubscribe").val();
                if (reg.test(address) == false) {
                    showSubError('Enter Valid Email id');
                    return false;
                }
                else {
                    $("#errorNewslwtter").hide();
                    submiSubscribetData();
                    window.location.href = "#DivSubscribe";
                }
            }
        });
    });

    function showSubError(strMsg) {
        $("#errorNewslwtter").show('slow');
        $("#errorNewslwtter").empty();
        $("#errorNewslwtter").removeClass('success').addClass('error');
        $("#errorNewslwtter").html(strMsg);
    }

    function showSubSuccess(strMsg) {
        $("#errorNewslwtter").show('slow');
        $("#errorNewslwtter").empty();
        $("#errorNewslwtter").removeClass('error').addClass('success');
        $("#errorNewslwtter").html(strMsg);
    }

    function submiSubscribetData() {
        var user = $('#ucFooter_txtSubscribe').val();

        $.ajax({
            type: "POST",
            url: '/VidyaVihar/ajax/ajaxCall.aspx',
            data: "email=" + user + "&action=Subscribe",
            cache: false,
            success: function(result) {
                if (result == "success") {
                    showSubSuccess('Thank you for subscribing to our newsletter');
                    $('#ucFooter_txtSubscribe').val('');
                    return true;
                } else {
                    showSubError('Record Exists');
                    $('#ucFooter_txtSubscribe').val('');
                    return true;
                }
                $('#ucFooter_txtSubscribe').val('');
            }
        });
    }

</script>

